<script>
// ***************************** DATA DEFAULT ******************************
    //list province
    $.getJSON('/api/province/get-all').then(resp => {
        resp.list.forEach(item => {
            $('#getselect_province').append('<option value="' + item.id + '">' +
                item.name + '</option>')
        })
    });
    // **************************************************************************
    // ***************************** SELECT VALUE *******************************
    function select_province() {
        var id_province = $("#getselect_province").val();
        $("#getselect_district").empty();

        $.getJSON('/api/district/getWithId/' + id_province).then(resp => {
            resp.list.forEach(item => {
                $('#getselect_district').append('<option value="' + item.id + '">' +
                    item.name + '</option>')
            })
        });
    }

    function select_district() {
        var id_district = $("#getselect_district").val();
        $("#getselect_commune").empty();

        $.getJSON('/api/commune/getWithId/' + id_district).then(resp => {
            resp.list.forEach(item => {
                console.log(item),
                $('#getselect_commune').append('<option value="' + item.id + '">' +
                    item.name + '</option>')
            })
        });
    }
    // **************************************************************************

</script>