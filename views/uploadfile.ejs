<script>
    // ***************************** DATA DEFAULT ******************************
    //list Style-Map
    $.getJSON('/api/type-map/get-all').then(resp => {
        resp.list.forEach(item => {
            $('#getselect_typemap').append('<option value="' + item.id + '">' +
                item.name + '</option>')
        })
    });

    //list province
    $.getJSON('/api/province/get-all').then(resp => {
        resp.list.forEach(item => {
            $('#getselect_province').append('<option value="' + item.id + '">' +
                item.name + '</option>')
        })
    });
    // **************************************************************************
    // ***************************** SELECT VALUE *******************************


    function select_province() {
        var id_province = $("#getselect_province").val();
        $("#getselect_district").empty();
        $("#getselect_commune").empty();

        $.getJSON('/api/district/getWithId/' + id_province).then(resp => {
            resp.list.forEach(item => {
                $('#getselect_district').append('<option value="' + item.id + '">' +
                    item.name + '</option>')
            })
        });
    }

    function select_district() {
        var id_district = $("#getselect_district").val();
        $("#getselect_commune").empty();

        $.getJSON('/api/commune/getWithId/' + id_district).then(resp => {
            resp.list.forEach(item => {
                console.log(item),
                    $('#getselect_commune').append('<option value="' + item.id + '">' +
                        item.name + '</option>')
            })
        });
    }
    // **************************************************************************

    //upload form file
    const upload = function () {
        var id_commune = $('#getselect_commune').val();
        var id_typemap = $('#getselect_typemap').val();

        if (id_commune == null) {
            alert("Vui lòng chọn xã!");
            return;
        }

        var file = document.getElementById('file-upload');
        if (file.files.length == 0) {
            alert("Vui lòng chọn file");
        } else {
            var formdata = new FormData();     
            formdata.append("file", file.files[0]);
            var requestOptions = {
                method: 'POST',
                body: formdata,
                redirect: 'follow'
            };
            fetch("/api/uploadfile", requestOptions)
                .then(response => response.text())
                .then(result => {
                    console.log(JSON.parse(result));
                    var tileset = JSON.parse(result).tileset;
                    //get item tileset
                    var requestOptions = {
                        method: 'GET',
                        redirect: 'follow'
                    };
                    console.log('>>check 1', tileset);
                    fetch("/api/getItemTileset/" + tileset, requestOptions)
                        .then(response => response.text())
                        .then(result => {
                            var data = JSON.parse(result);

                            var myHeaders = new Headers();
                            myHeaders.append("Content-Type", "application/json");
                            myHeaders.append("Cookie",
                                "connect.sid=s%3AeP7klvJzkC77lTlQChP85p53bYXWZSdr.I0hoRBSuVrJLp0GZqe%2B4OiA5B%2BwAKZffj%2B6gQ76wc8s"
                            );
                            console.log('>>check 2');
                            var new_date = moment(new Date).format('YYYY-MM-DD HH:mm:ss');
                            var des;
                            if (data.description) {
                                des = data.description;
                            } else {
                                des = 'Dữ liệu mới';
                            }
                            console.log('>>check 3');
                            const name_map = `${data.name.split("-")[1]}`;
                            console.log(name_map);
                            var raw = JSON.stringify({
                                "id": data.id, //or id
                                "name": name_map,
                                "visibility": data.visibility,
                                "createdated": new_date,
                                "description": des,
                                "filesize": data.filesize,
                                "status": '1',
                                "zoom": data.center[2],
                                "lags": String(data.center[1]),
                                "lngs": String(data.center[0]),
                                "type": data.type,
                                "idcommune": id_commune,
                                "idtypemap": id_typemap,
                            });
                            console.log('>>check 4');
                            var requestOptions = {
                                method: 'POST',
                                headers: myHeaders,
                                body: raw,
                                redirect: 'follow'
                            };
                            // create map
                            fetch("/api/create-data-map", requestOptions)
                                .then(response => {
                                    if (response.status == 200) {
                                        alert("Lưu thành công!")
                                        window.location.href = "/home";
                                    } else {
                                        alert("Lưu thất bại!")
                                        window.location.href = "/home/upload";
                                    }
                                }).catch(error => console.log('error', error));
                        }).catch(error => console.log('error', error));
                }).catch(error => console.log('error', error));
        }
    }
</script>